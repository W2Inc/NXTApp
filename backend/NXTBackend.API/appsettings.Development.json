{
    "Logging": {
        "LogLevel": {
            "Default": "Debug",
            "Microsoft.AspNetCore": "Debug"
        }
    },
    "Quartz": {
        "quartz.scheduler.instanceName": "Quartz ASP.NET Core Sample Scheduler"
    },
    "Keycloak": {
        "ssl-required": "external",
        "verify-token-audience": false,
        "confidential-port": 0,
        "policy-enforcer": {
            "credentials": {}
        }
    },
    "GitRemote": {
        "DefaultBranch": "main",
        "ApiUrl": "https://git.hamette.net/api/v1/",
        "Template": "demo/template"
    },
    "MailSettings": {
        "Host": "smtp.resend.com",
        "DefaultCredentials": false,
        "Port": 587,
        "UserName": "resend"
    },
    "NetworkSettings": {
        "AllowedIpRanges": [
            "10.1.0.0/16",
            "10.2.0.0/16",
            "10.3.0.0/16",
            "10.4.0.0/16"
        ]
    }
}
